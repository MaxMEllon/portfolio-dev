    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-act": "^1.0.0",
    "redux-actions": "^0.12.0",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.1",
    "reset.css": "^2.0.2",
