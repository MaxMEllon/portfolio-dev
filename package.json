{
  "name": "maxmellon-github-io",
  "main": "lib",
  "files": [
    "lib",
    "release"
  ],
  "scripts": {
    "start": "karma start",
    "test": "karma start --single-run",
    "cover": "abby test, report",
    "lint": "eslint src test",
    "prerelease": "npm run build",
    "release": "cp -r bundle deploy && cd deploy && git add . && git commit -m 'Auto commit' && git push",
    "build": "abby webpack --env",
    "dev": "abby dev:* --parse parallel",
    "dev:env": "cross-env NODE_ENV=development",
    "dev:server": "node ./webpack.dev.server.js" ,
    "dev:open": "wait-on http://localhost:8080 && opener http://localhost:8080/webpack-dev-server"
  },
  "abigail": {
    "plugins": {
      "watch": false,
      "parse": "raw",
      "launch": "force"
    }
  },
  "devDependencies": {
    "abigail": "^1.9.1",
    "animate.css": "^3.5.1",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "codeclimate-test-reporter": "^0.3.3",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-github-releaser": "^1.1.3",
    "cross-env": "^3.0.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "pascal-case": "^2.0.0",
    "power-assert": "^1.4.1",
    "react": "^15.3.1",
    "react-addons-deep-compare": "0.0.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.3.1",
    "react-fa": "^4.1.2",
